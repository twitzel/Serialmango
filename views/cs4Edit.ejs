<body class = 'body' onresize="resize_canvas()">
<div id = "title" >

</div>

<div id = "canvasDiv">
    <h3 id = "in" > Incoming Data  </h3>
    <button id = "loadButton"  onmousedown = "loadclick()">Load Cue File </button>
    <button id = "saveButton"  onmousedown = "saveclick()">Save Cue File</button>
    <h3 id = "out" >Outgoing Data</h3>
    Zoom: 0<input type="range" name="zoom" step = 0.25 value = 0 max = 99.75 onchange="zoomChange(this.value)" oninput="zoomChange(this.value)">100
    Location: 0<input type="range" name="zoom" step = 0.25 value = 50 onchange="locationChange(this.value)" oninput="locationChange(this.value)">100
    <canvas id="mainCanvas" width= 1000px height=300px></canvas>
    <canvas id="zoomCanvas" width= 1000px height=400px></canvas>

</div>
